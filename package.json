  "description": "This library provides method to get japanese address from zipcode.",
  "keywords": [
    "github",
    "github actions"
  ],
  "homepage": "https://github.com/technote-space/zipcode2address-jp",
  "bugs": {
    "url": "https://github.com/technote-space/zipcode2address-jp/issues"
  },
